{
    "name": "pyrrah/gravatar-bundle",
    "description": "The simple way to use Gravatar API for Symfony 5.",
    "homepage": "https://github.com/Pyrrah/GravatarBundle",
    "keywords": ["pyrrah", "gravatar", "picture gravatar", "gravatar symfony3", "gravatar symfony4", "gravatar symfony5"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [{
        "name" : "Thibault Duplessis",
        "email" : "thibault.duplessis@gmail.com"
    },
    {
        "name" : "Henrik BjÃ¸rnskov",
        "email" : "henrik@bjrnskov.dk"
    },
    {
        "name": "Pierre-Yves Dick",
        "email": "hello@pierreyvesdick.fr"
    }],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.3",
        "symfony/framework-bundle" : "^3.4|^4.0|^5.0|^6.0",
        "symfony/templating": "^3.4|^4.0|^5.0|^6.0"
    },
    "require-dev": {
        "twig/extensions": "1.0.*",
        "symfony/phpunit-bridge": "^4.3|^5.0|^6.0"
    },
    "suggest" : {
        "twig/twig" : "For gravatar twig extension usage"
    },
    "conflict": {
        "twig/twig": "<1.12"
    },
    "autoload": {
        "psr-4": { "Pyrrah\\GravatarBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Pyrrah\\GravatarBundle\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    }
}