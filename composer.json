{
    "name": "pyrrah/gravatar-bundle",
    "description": "The simple way to use Gravatar API for Symfony 6 and Symfony 7",
    "homepage": "https://github.com/Pyrrah/GravatarBundle",
    "keywords": ["pyrrah", "gravatar", "picture gravatar", "gravatar symfony6", "gravatar symfony7"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [{
        "name" : "Thibault Duplessis",
        "email" : "thibault.duplessis@gmail.com"
    },
    {
        "name" : "Henrik BjÃ¸rnskov",
        "email" : "henrik@bjrnskov.dk"
    },
    {
        "name": "Pierre-Yves Dick",
        "email": "hello@pierreyvesdick.fr",
        "homepage": "https://github.com/Pyrrah"
    }],
    "support": {
        "issues": "https://github.com/Pyrrah/GravatarBundle/issues"
    },
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle" : "^6.4|^7.0",
        "symfony/templating": "^6.4|^7.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.1|^7.0"
    },
    "suggest" : {
        "twig/twig" : "For Gravatar twig extension usage"
    },
    "autoload": {
        "psr-4": { "Pyrrah\\GravatarBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Pyrrah\\GravatarBundle\\Tests\\": "tests/" }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "v1.5.x-dev"
        }
    }
}